specpath readline.smackspec

target mlton
    sources
    	src/smbt.mlb
	src/go.sml
    end
    
    option compiler = mlton 
    option output = bin/smbt
end

target smlnj
    sources
        src/smbt.cm
    end

    option compiler = smlnj
    option exportFn = Smbt.main
    option heapImg = bin/.heapimg
    option output = bin/smbt-nj
end
